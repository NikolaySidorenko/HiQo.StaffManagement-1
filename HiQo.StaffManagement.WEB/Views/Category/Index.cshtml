@model IEnumerable<HiQo.StaffManagement.BL.Domain.Entities.CategoryDto>
@{
    ViewBag.Title = "Categories";
}
<h2>Categories</h2>
<div>
    <table class="table">
        <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Department</th>
        </tr>
        </thead>
        @foreach (var obj in Model)
        {
            <tr>
                <td>
                    <p>@obj.CategoryId</p>
                </td>
                <td>
                    <p>@obj.Name</p>
                </td>
                <td>
                    <p>@obj.Department.Name</p>
                </td>
            </tr>
        }
    </table>
</div>
