@model IEnumerable<HiQo.StaffManagement.BL.Domain.Entities.RoleDto>
@{
    ViewBag.Title = "Roles";
}
<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Name</th>
            </tr>
        </thead>
        @foreach (var obj in Model)
        {
            <tr>
                <td>
                    <p>@obj.RoleId</p>
                </td>
                <td>
                    <p>@obj.Name</p>
                </td>
            </tr>
        }
    </table>
</div>